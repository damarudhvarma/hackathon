{
    "rewrites": [
      {
        "source": "/(.*)",    // Match all routes
        "destination": "/"     // Redirect them to index.html
      }
    ],
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"   
        }
      }
    ]
  }
  